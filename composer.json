    "license" : "GPL-3.0",
    "authors" : [{
         "name": "Stanislav WEB",
         "email": "stanisov@gmail.com",
         "homepage": "http://reds.com",
         "role": "Developer"
    }],

    "require" : {
         "php" : ">=5.4.0"
    },

    "autoload": {
        "psr-4": {
            "ULogin\\": "src/ULogin"
        },
        "classmap": ["phpunit/src/ULogin"]
